// declaração de uma função chamada incrementa
function incrementa() {
    // mensagem que será exibida no console quando a função for chamada
    console.log("inside the function");
}

// chamada da função incrementa
incrementa();

let valor = 20



// declaração de uma função chamada increment
function increment(a, b) {
    // somando os parâmetros 'a' e 'b'
    let valor = a + b;

    // exibindo o resultado no console
    console.log(valor);
}

// chamada da função increment com os valores de 'a' e 'b' como argumentos
increment(3, 29);
